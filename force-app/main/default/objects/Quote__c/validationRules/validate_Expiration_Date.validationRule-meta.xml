<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>validate_Expiration_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    RecordType.Name = &apos;Quote&apos;,
    ISBLANK(ExpirationDate__c)
)</errorConditionFormula>
    <errorMessage>Enter Expiration Date</errorMessage>
</ValidationRule>
